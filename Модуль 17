per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
money = int(input("Введите сумму, которую планируете положить под проценты:"))
s1 = int(money * per_cent.get('ТКБ')/100)
s2 = int(money * per_cent.get('СКБ')/100)
s3 = int(money * per_cent.get('ВТБ')/100)
s4 = int(money * per_cent.get('СБЕР')/100)
desposit = [s1, s2, s3, s4]
print(desposit)
print("Максимальная сумма, которую выможете заработать -", max(desposit))
